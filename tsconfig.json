{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "types": ["vitest/globals"]
  },
  "include": ["src"],
}
